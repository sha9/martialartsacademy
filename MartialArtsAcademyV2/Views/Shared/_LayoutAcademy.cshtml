@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <title>Martial Arts Academy | Om MAA</title>
    <link href="~/Content/gympack/bootstrap.css" rel='stylesheet' type='text/css' />
    <link href="~/Content/gympack/style.css" rel='stylesheet' type='text/css' />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700,800' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Open+Sans+Condensed:300,700' rel='stylesheet' type='text/css'>
    <script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false); function hideURLbar(){ window.scrollTo(0,1); } </script>
    <script src="~/Scripts/gympack/jquery.min.js"></script>
    @Scripts.Render("~/bundles/jquery")
    <script src="~/Scripts/gympack/jquery.min.js"></script>
    <!-- grid-slider -->
    <script type="text/javascript" src="~/Scripts/gympack/jquery.mousewheel.js"></script>
    <script type="text/javascript" src="~/Scripts/gympack/jquery.contentcarousel.js"></script>
    <script type="text/javascript" src="~/Scripts/gympack/jquery.easing.1.3.js"></script>
    <!-- //grid-slider -->
    <!---calender-style---->
    <link rel="stylesheet" href="~/Content/gympack/jquery-ui.css" />
    <!---//calender-style---->
</head>
<body>
    @Html.Partial("_HeaderBottom")
    @RenderSection("scripts", required: false)
        @RenderBody()
    @Html.Partial("_GalleryPartner")
    <div class="footer-bottom">
        @Html.Partial("_FooterBottom")
    </div>
    <div class="copyright">
        @Html.Partial("_Copyright")
    </div>
</body>
</html>
